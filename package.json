{
  "name": "vscodefy",
  "displayName": "vscodefy",
  "description": "",
  "version": "0.0.1",
  "publisher": "vscode-spotify",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "vscodefy.next",
        "title": "Spotify: Play Next Song"
      },
      {
        "command": "vscodefy.previous",
        "title": "Spotify: Play Previous Song"
      },
      {
        "command": "vscodefy.play",
        "title": "Spotify: Play Song"
      },
      {
        "command": "vscodefy.pause",
        "title": "Spotify: Pause Song"
      },
      {
        "command": "vscodefy.login",
        "title": "Spotify: Login"
      },
      {
        "command": "vscodefy.getCode",
        "title": "Spotify: Set OAuth Code"
      },
      {
        "command": "vscodefy.pickDevice",
        "title": "Spotify: Pick a device"
      },
      {
        "command": "vscodefy.logout",
        "title": "Spotify: Logout"
      }
    ]
  },
  "scripts": {
    "linter": "eslint src",
    "build": "babel src --source-maps -D -d dist",
    "vscode:prepublish": "npm run build",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.18"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "pubsub-js": "^1.6.0",
    "rxjs": "^6.2.2"
  }
}
